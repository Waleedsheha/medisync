# Environment Variables Template for MediSync
# Copy this file to .env and fill in your actual API keys

# DeepSeek API
# Get your key from: https://platform.deepseek.com/api_keys
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_BASE_URL=https://api.deepseek.com/chat/completions

# HuggingFace API
# Get your key from: https://huggingface.co/settings/tokens
HUGGINGFACE_API_KEY=your_huggingface_token_here
HUGGINGFACE_BASE_URL=https://api-inference.huggingface.co/models
HUGGINGFACE_DEFAULT_MODEL=MiniMaxAI/MiniMax-M2.1

# Supabase Backend
# Get these from: https://supabase.com/dashboard/project/_/settings/api
# For local development: http://localhost:54321 and your local anon key
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
